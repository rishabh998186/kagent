[project]
name = "{{.Name}}"
version = "0.1.0"
description = "{{.Name}} agent using LangGraph"
readme = "README.md"
requires-python = ">=3.13"
dependencies = [
    "kagent-langgraph",
    "langgraph>=0.2.0",
    "langchain-core>=0.3.0",
    "langchain-google-genai>=1.0.0",
    "langgraph-checkpoint-sqlite>=2.0.0",
    "langsmith[otel]>=0.4.30",
    "langchain-mcp>=0.1.0",
    "anthropic>=0.18.0",
    "openai>=1.0.0",
    "google-genai>=0.6.0",
]

[build-system]
requires = ["setuptools>=61.0", "wheel"]
build-backend = "setuptools.build_meta"

[project.scripts]
{{.Name}} = "{{.Name}}.cli:main"
