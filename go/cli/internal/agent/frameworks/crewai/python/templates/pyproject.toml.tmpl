[project]
name = "{{.Name}}"
version = "0.1.0"
description = "{{.Name}} agent using CrewAI"
readme = "README.md"
requires-python = ">=3.12"
dependencies = [
    "kagent-crewai",
    "crewai>=0.86.0,<1.0.0",
]

[build-system]
requires = ["setuptools>=61.0", "wheel"]
build-backend = "setuptools.build_meta"

[project.scripts]
{{.Name}} = "{{.Name}}.crew:main"
